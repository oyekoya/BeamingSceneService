<a name="l00473"></a>00473         
<a name="l00474"></a>00474         var joy_y = GetTrackerJoy(1).y;
<a name="l00475"></a>00475         <span class="keywordflow">if</span>(abs(joy_y) &gt; joystick_dead_zone){
<a name="l00476"></a>00476                 <a class="code" href="client_8s3d.html#af8e4c45bdecbc91a87c9bc1b647c0757">CamPos</a> = CameraGetPosition();
<a name="l00477"></a>00477                 var temp = [0,0,-joy_y];
<a name="l00478"></a>00478                 norm(temp);
<a name="l00479"></a>00479                 temp = VectorRotate(-<a class="code" href="client_8s3d.html#a343c04ec37efc202b6b900c32d2bb758">angolo</a>, [0,1,0], temp);
<a name="l00480"></a>00480                 temp = VectorRotate(wand_xrot, [1,0,0], temp);
