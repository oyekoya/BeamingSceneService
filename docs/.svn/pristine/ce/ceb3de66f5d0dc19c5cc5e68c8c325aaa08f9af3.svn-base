<a name="l00387"></a><a class="code" href="client_8s3d.html#ac60553ac4bd434fb73292aee4c6b6b2e">00387</a> <span class="keyword">function</span> <a class="code" href="client_8s3d.html#ac60553ac4bd434fb73292aee4c6b6b2e">slerp</a>(qa, qb, t) 
<a name="l00388"></a>00388 {
<a name="l00389"></a>00389         <span class="comment">// quaternion to return</span>
<a name="l00390"></a>00390         var qm = Vector(4);
<a name="l00391"></a>00391         <span class="comment">// Calculate angle between them.</span>
<a name="l00392"></a>00392         var  cosHalfTheta = qa.w * qb.w + qa.x * qb.x + qa.y * qb.y + qa.z * qb.z;
<a name="l00393"></a>00393         <span class="comment">// if qa=qb or qa=-qb then theta = 0 and we can return qa</span>
<a name="l00394"></a>00394         <span class="keywordflow">if</span> (abs(cosHalfTheta) &gt;= 1.0){
<a name="l00395"></a>00395                 qm.w = qa.w;qm.x = qa.x;qm.y = qa.y;qm.z = qa.z;
<a name="l00396"></a>00396                 <span class="keywordflow">return</span> qm;
<a name="l00397"></a>00397         }
<a name="l00398"></a>00398         <span class="comment">// Calculate temporary values.</span>
<a name="l00399"></a>00399         var halfTheta = acos(cosHalfTheta);
<a name="l00400"></a>00400         var sinHalfTheta = sqrt(1.0 - cosHalfTheta*cosHalfTheta);
<a name="l00401"></a>00401         <span class="comment">// if theta = 180 degrees then result is not fully defined</span>
<a name="l00402"></a>00402         <span class="comment">// we could rotate around any axis normal to qa or qb</span>
<a name="l00403"></a>00403         <span class="keywordflow">if</span> (abs(sinHalfTheta) &lt; 0.001){ <span class="comment">// fabs is floating point absolute</span>
<a name="l00404"></a>00404                 qm.w = (qa.w * 0.5 + qb.w * 0.5);
<a name="l00405"></a>00405                 qm.x = (qa.x * 0.5 + qb.x * 0.5);
<a name="l00406"></a>00406                 qm.y = (qa.y * 0.5 + qb.y * 0.5);
<a name="l00407"></a>00407                 qm.z = (qa.z * 0.5 + qb.z * 0.5);
<a name="l00408"></a>00408                 <span class="keywordflow">return</span> qm;
<a name="l00409"></a>00409         }
<a name="l00410"></a>00410         var ratioA = sin((1 - t) * halfTheta) / sinHalfTheta;
<a name="l00411"></a>00411         var ratioB = sin(t * halfTheta) / sinHalfTheta; 
<a name="l00412"></a>00412         <span class="comment">//calculate Quaternion.</span>
<a name="l00413"></a>00413         qm.w = (qa.w * ratioA + qb.w * ratioB);
<a name="l00414"></a>00414         qm.x = (qa.x * ratioA + qb.x * ratioB);
<a name="l00415"></a>00415         qm.y = (qa.y * ratioA + qb.y * ratioB);
<a name="l00416"></a>00416         qm.z = (qa.z * ratioA + qb.z * ratioB);
<a name="l00417"></a>00417         <span class="keywordflow">return</span> qm;
<a name="l00418"></a>00418 }
<a name="l00419"></a>00419 
<a name="l00420"></a><a class="code" href="client_8s3d.html#a3d56bdf90cf0b023b65fae73da903a23">00420</a> <span class="keyword">function</span> <a class="code" href="client_8s3d.html#a3d56bdf90cf0b023b65fae73da903a23">AxisAngleToQuat</a>(a1, q1) {
<a name="l00421"></a>00421   var s = sin(a1[3]/2);
<a name="l00422"></a>00422   q1[0] = a1[0] * s;
<a name="l00423"></a>00423   q1[1] = a1[1] * s;
<a name="l00424"></a>00424   q1[2] = a1[2] * s;
<a name="l00425"></a>00425   q1[3] = cos(a1[3]/2);
<a name="l00426"></a>00426 }
<a name="l00427"></a>00427 
<a name="l00428"></a>00428 <span class="comment">// return euler angles in radians from a rotation matrix</span>
<a name="l00429"></a><a class="code" href="client_8s3d.html#adfba454a5dff92da0ecd02f37bda2381">00429</a> <span class="keyword">function</span> <a class="code" href="client_8s3d.html#adfba454a5dff92da0ecd02f37bda2381">matrixToAngles</a>(m, a){
<a name="l00430"></a>00430         <span class="keywordflow">if</span>(abs(m[1]) &gt;= 0.9999)
<a name="l00431"></a>00431         {
<a name="l00432"></a>00432                 a[0] = atan2(-m[4],m[8]);
<a name="l00433"></a>00433                 a[1] = 0;
<a name="l00434"></a>00434                 a[2] = <a class="code" href="util_math_8h.html#aa08a577393243b86dfd2a97e61443673">PI</a>/2;
<a name="l00435"></a>00435                 <span class="keywordflow">return</span> a;       
<a name="l00436"></a>00436         }
<a name="l00437"></a>00437         a[0] = atan2(-m[9], m[5]);
<a name="l00438"></a>00438         a[1] = atan2(-m[2], m[0]);
<a name="l00439"></a>00439         a[2] = asin(m[1]);
<a name="l00440"></a>00440         <span class="keywordflow">return</span> a;
<a name="l00441"></a>00441 }
